{
  "manifest_version": 2,
  "name": "Spoilern't",
  "version": "1.0",
  "description": "Hides descriptions on Netflix",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.netflix.com/*"],
      "js": ["spoilernt.js"]
    }
  ]
}
